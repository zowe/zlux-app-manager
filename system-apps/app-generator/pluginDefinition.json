{
  "identifier": "org.zowe.zlux.generator",
  "apiVersion": "1.0.0",
  "pluginVersion": "1.0.0",
  "pluginType": "application",
  "webContent": {
    "framework": "iframe",
    "launchDefinition": {
      "pluginShortNameKey": "PluginName",
      "pluginShortNameDefault": "Zowe App Generator",
      "imageSrc": "assets/icon.png"
    },
    "descriptionKey": "PluginDescription",
    "descriptionDefault": "App Generator for Zowe",
    "defaultWindowStyle": {
      "width": 1600,
      "height": 800
    }
  },
  "dataServices": [
    {
      "type": "router",
      "name": "gen",
      "initializerLookupMethod": "external",
      "fileName": "starter.js",
      "routerFactory": "genRouter",
      "dependenciesIncluded": true,
      "version": "1.0.0"
    }
  ]
}
